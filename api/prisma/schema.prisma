datasource db {
  provider = "sqlite"
}

generator client {
  provider = "prisma-client-js"
}

model users {
  id          Int       @id   @default(autoincrement())
  created_at  DateTime  @default(now())
  updated_at  DateTime  @updatedAt
  name        String
  email       String
  password    String
  markers     markers[]
}

model markers {
  id            Int       @id   @default(autoincrement())
  created_at    DateTime  @default(now())
  updated_at    DateTime  @updatedAt
  lat           Float
  lon           Float
  user_id       Int
  place_id      Int?
  display_name  String?
  building      String?
  city          String?
  state         String?
  country       String?
  postcode      String?
  user          users   @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
}